    "build:swagger:watch": "webpack --config webpack-swagger.config.js",
    "sls": "serverless  --config serverless.yml",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "api": "ts-node -P ./tsconfig.json build-api.ts",
    "api:watch": "nodemon --watch 'src' --ext .ts -i app.controller.ts --exec \"npm run api\"",
